<!DOCTYPE html>
<html class="no-js" lang="en" data-theme="dark">
<head>
  <meta charset="utf-8" />
  <meta http-equiv="x-ua-compatible" content="ie=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title><%= htmlWebpackPlugin.options.templateParameters.pageTitle %></title>
  <!-- all stylesheets loaded here -->
  <% for (const styleName of htmlWebpackPlugin.files.css) { %>
  <link rel="stylesheet" href="<%= styleName %>" />
  <% } %>
</head>
<body>
  <div id="title" class="title-bar show-for-small-only">
    <div class="title-bar-left">
      <span class="title-bar-title"><%= htmlWebpackPlugin.options.templateParameters.pageTitle %></span>
    </div>
    <div class="title-bar-right">
      <button type="button" class="primary button" id="menuButton" data-toggle="menuWrapper">Menu</button>
    </div>
  </div>
  <div id="pageWrapperOuter" class="off-canvas-wrapper">
    <div id="pageWrapperInner">
      <div id="menuWrapper" class="off-canvas" data-off-canvas data-close-on-click="true" data-in-canvas-on="medium">
        <ul id="menu">
          <li><span class="menu-title"><%= htmlWebpackPlugin.options.templateParameters.pageTitle %></span></li>
          <li>
            <label id="darkModeLabel" for="darkModeSwitch">Toggle dark mode</label>
            <div class="switch" id="darkModeControl">
              <input class="switch-input" id="darkModeSwitch" name="darkModeSwitch" aria-labelledby="darkModeLabel" type="checkbox">
              <label class="switch-paddle" for="darkModeSwitch"></label>
            </div>
          </li>
        </ul>
      </div>
      <div id="app" class="off-canvas-content" data-off-canvas-content>
        <div id="controlsWrapper">
          <label id="lightnessLabel" for="lightnessThumb">Lightness parameter</label>
          <div id="lightnessControl" class="slider" data-slider data-initial-start="80" data-start="0" data-end="100" data-step="2" role="presentation">
            <span id="lightnessThumb" name="lightnessThumb" aria-labelledby="lightnessLabel" class="slider-handle" data-slider-handle role="slider" tabindex="0"></span>
            <span class="slider-fill" data-slider-fill role="presentation"></span>
            <input type="hidden" id="lightnessInput">
          </div>
        </div>
        <div id="renderWrapper">
          <img id="colors" src="" />
          <div id="zoom"></div>
        </div>
      </div>
    </div>
  </div>
  <!-- vendor scripts always loaded first -->
  <% for (const scriptName of htmlWebpackPlugin.files.js) { %>
  <% if (scriptName != `${htmlWebpackPlugin.options.templateParameters.entryName}.bundle.js`) { %>
  <script src="<%= scriptName %>" type="text/javascript"></script>
  <% } %>
  <% } %>
  <!-- page script always loaded last -->
  <script src="<%= htmlWebpackPlugin.options.templateParameters.entryName %>.bundle.js" type="text/javascript"></script>
</body>
</html>
